{
    "name": "@pablomagalhaes/component-library",
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/pablomagalhaes"
    },
    "version": "0.0.6",
    "description": "A component library for getting people started with easy to re-use components for everyday projects.",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rm -rf dist/ && prettier --write src/ && npm run build:esm && npm run build:cjs",
        "build:esm": "tsc",
        "build:cjs": "tsc --module CommonJS --outDir dist/cjs"
    },
    "author": "Thiago Magalh√£es",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^17.0.45",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "@types/react-table": "^7.7.14",
        "@types/react-window": "^1.8.5",
        "@types/styled-components": "^5.1.26",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "typescript": "^4.5.2"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-feather": "^2.0.10",
        "react-table": "^7.8.0",
        "react-window": "^1.8.8",
        "reactstrap": "^9.1.5",
        "styled-components": "^5.3.6"
    }
